    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome SKIA",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--dart-define",
                "FLUTTER_WEB_USE_SKIA=true"
            ]
        },
        {
            "name": "Chrome web-renderer",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-renderer=html"
            ]
        }
    ]
}